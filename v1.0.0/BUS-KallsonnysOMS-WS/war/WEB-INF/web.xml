<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>KallsonnysOMS-WS</display-name>

	<servlet>
		<display-name>CustomerServices</display-name>
		<servlet-name>CustomerServices</servlet-name>
		<servlet-class>org.kallsonys.oms.ws.customerservices.CustomerServicesImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CustomerServices</servlet-name>
		<url-pattern>/CustomerServices</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>ProductServices</display-name>
		<servlet-name>ProductServices</servlet-name>
		<servlet-class>org.kallsonys.oms.ws.productservices.ProductServicesImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProductServices</servlet-name>
		<url-pattern>/ProductServices</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>CampaignServices</display-name>
		<servlet-name>CampaignServices</servlet-name>
		<servlet-class>org.kallsonys.oms.ws.campaignservices.CampaignServicesImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CampaignServices</servlet-name>
		<url-pattern>/CampaignServices</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>OrderServices</display-name>
		<servlet-name>OrderServices</servlet-name>
		<servlet-class>org.kallsonys.oms.ws.orderservices.OrderServicesImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>OrderServices</servlet-name>
		<url-pattern>/OrderServices</url-pattern>
	</servlet-mapping>
</web-app>